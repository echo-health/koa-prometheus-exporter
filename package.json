{
  "name": "@echo-health/koa-prometheus-exporter",
  "version": "0.0.7",
  "main": "src/index.js",
  "license": "MIT",
  "engines": {
    "node": ">=6.10.2"
  },
  "scripts": {
    "test": "jest --env node",
    "lint": "eslint .",
    "postinstall": "nsp check",
    "format": "prettier --write 'src/**/*.js'",
    "format:check": "prettier --list-different 'src/**/*.js'"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "coverageReporters": [
      "json",
      "lcov",
      "text-summary"
    ]
  },
  "prettier": {
    "trailingComma": "es5",
    "singleQuote": true,
    "tabWidth": 4
  },
  "dependencies": {
    "debug": "3.1.0",
    "nsp": "3.1.0",
    "prom-client": "^10.1.1",
    "prometheus-gc-stats": "^0.5.0"
  },
  "devDependencies": {
    "eslint": "4.7.1",
    "eslint-config-airbnb-base": "12.0.0",
    "eslint-config-prettier": "2.5.0",
    "eslint-plugin-import": "2.7.0",
    "jest": "21.1.0",
    "prettier": "1.7.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:echo-health/koa-prometheus-exporter.git"
  },
  "author": "Echo",
  "contributors": [
    {
      "name": "Ryan McGrath",
      "email": "ryan.mcgrath@echo.co.uk"
    }
